{
  "id": "com.gmail.cse.ari007.d3forcebubble",
  "name": "organigramm",
  "version": "0.0.0",
  "description": "Organigramm",
  "newInstancePrefix": "OGRAMM",
  "vendor": "Organigramm",

  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-gmail-cse-ari007-d3forcebubble",
      "url": "https://github.com/LauraScha/Organigramm/blob/master/Organigramm.js",
      "integrity": "",
      "ignoreIntegrity": true
    }

  ],

  "properties": {
    "data": {
      "type": "string",
      "description": "Data in JSON format",
      "default": "[{\"name\":\"Label 1\",\"size\":850.84, \"color\":50.7, \"xvalue\":100},{\"name\":\"Label 2\",\"size\":3308.76, \"color\":10, \"xvalue\":20.5}]"
    }

  },
  "methods": {
    "setData": {
      "description": "Add Node to Organigramm",
      "parameters": [
        {
          "name": "newData",
          "type": "string",
          "description": "New Data of the Chart"
        }
      ],
      "body": "this.data = newData;"
    }

  },

  "events": {
    "onClick": {
      "description": "Called when the user click on Item."
    }

  }

}

